{
    "name": "admindashboard",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
        "test": "vitest",
        "lint": "prettier --check . && eslint .",
        "format": "prettier --write .",
        "precommit": "pnpx pretty-quick --staged",
        "prepare": "cd .. && husky AdminDashboard/.husky",
        "updateapi-node": "pnpx @openapitools/openapi-generator-cli generate -i api/main.yaml -o src/lib/generated/ -g typescript-fetch",
        "updateapi": "openapi-generator generate -i api/main.yaml -o src/lib/generated/ -g typescript-fetch"
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.11.0",
        "@smui/button": "7.0.0-beta.18",
        "@smui/data-table": "7.0.0-beta.18",
        "@smui/linear-progress": "7.0.0-beta.18",
        "@sveltejs/adapter-auto": "^3.1.1",
        "@sveltejs/kit": "^2.5.2",
        "@sveltejs/vite-plugin-svelte": "^3.0.2",
        "@types/eslint": "^8.56.5",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.35.1",
        "flowbite": "^2.3.0",
        "flowbite-svelte": "^0.44.24",
        "git-format-staged": "^3.1.1",
        "husky": "^9.0.11",
        "prettier": "^3.2.5",
        "prettier-plugin-svelte": "^3.2.2",
        "pretty-quick": "^4.0.0",
        "svelte": "^4.2.12",
        "svelte-check": "^3.6.6",
        "svelte-routing": "^2.12.0",
        "typescript": "^5.3.3",
        "vite": "^5.1.5",
        "vitest": "^1.3.1"
    },
    "type": "module",
    "dependencies": {
        "js-cookie": "^3.0.5",
        "jwt-decode": "^4.0.0"
    }
}
