{
    "name": "studentportal",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
        "test": "vitest",
        "lint": "prettier --check . && eslint .",
        "format": "prettier --write .",
        "precommit": "pnpx pretty-quick --staged",
        "prepare": "cd .. && husky StudentPortal/.husky",
        "updateapi-node": "pnpx @openapitools/openapi-generator-cli generate -i api/main.yaml -o src/lib/generated/ -g typescript-fetch",
        "updateapi": "openapi-generator generate -i api/main.yaml -o src/lib/generated/ -g typescript-fetch",
        "prepare-theme": "pnpm run smui-theme-light && pnpm run smui-theme-dark",
        "smui-theme-light": "smui-theme compile static/smui.css -i src/theme -i node_modules",
        "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark -i node_modules"
    },
    "devDependencies": {
        "@openapitools/openapi-generator-cli": "^2.13.2",
        "@material/typography": "^14.0.0",
        "@smui/button": "7.0.0-beta.18",
        "@sveltejs/adapter-auto": "^3.2.0",
        "@sveltejs/kit": "^2.5.6",
        "@sveltejs/vite-plugin-svelte": "^3.1.0",
        "@types/eslint": "^8.56.10",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.37.0",
        "husky": "^9.0.11",
        "prettier": "^3.2.5",
        "prettier-plugin-svelte": "^3.2.3",
        "smui-theme": "7.0.0-beta.18",
        "svelte": "^4.2.15",
        "svelte-check": "^3.6.9",
        "typescript": "^5.4.5",
        "typescript-svelte-plugin": "^0.3.37",
        "vite": "^5.2.9",
        "vitest": "^1.5.0"
    },
    "type": "module",
    "dependencies": {
        "@material/theme": "^14.0.0",
        "@smui/button": "7.0.0-beta.18",
        "@sveltejs/adapter-vercel": "^5.3.0",
        "@smui/paper": "7.0.0-beta.18",
        "@smui/textfield": "7.0.0-beta.18",
        "js-cookie": "^3.0.5",
        "jwt-decode": "^4.0.0",
        "svelte-material-ui": "7.0.0-beta.18"
    }
}
